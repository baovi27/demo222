package com.poly;

/**
 *
 * @author Admin
 */
public class Vuong extends ChuNhat{
    double canh;

    public Vuong(double canh ) {
        super(canh,canh);
        this.canh = canh;
    }
    @Override
    public void xuat(){
        System.out.printf("Hình vuông có canh là%.1f",canh);
        System.out.printf("\nHình vuông có chu vi là %.1f, diện tích là %.1f",getChuVi(),getDienTich());
        System.out.println();
    }
}
